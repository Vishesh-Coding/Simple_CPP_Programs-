                                          //Check if a Number is an Armstrong Number

#include<iostream>
using namespace std;

int main ()
 {
     int n,r,t,s;
     cout << "enter a number ";
     cin >>n;
     t=n;
    
 while(t != 0)
 
    {
        r= t % 10;
        s= s + r*r*r;
        t= t / 10;
    }
 if (s == n)
        cout << "\n" <<n << " is an Armstrong number.";
    else
        cout << "\n" <<n << " is not an Armstrong number.";
    return 0;
 }
