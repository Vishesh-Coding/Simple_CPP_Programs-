// Swap two number without using two numbers

#include<iostream>
using namespace std;

int main ()
{
    int a,b;
    cout << "Enter first number : ";
    cin >> a;
    cout << "Enter second number : ";
    cin >> b;
    
    a=a+b;
    b=a-b;
    a=a-b;
    
 cout <<a <<b <<endl;    
    return 0;
}
